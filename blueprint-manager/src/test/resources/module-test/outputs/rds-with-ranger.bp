    {
      "name": "worker",
      "configurations": [
        {
          "hdfs-site": {
            "dfs.datanode.data.dir": "/hadoopfs/fs1/hdfs/datanode"
          }
        },
        {
          "yarn-site": {
            "yarn.nodemanager.local-dirs": "/hadoopfs/fs1/yarn/nodemanager",
            "yarn.nodemanager.log-dirs": "/hadoopfs/fs1/yarn/nodemanager/log"
          }
